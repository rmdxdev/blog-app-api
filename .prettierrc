{
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "semi": false,
  "tabWidth": 2,
  "printWidth": 100,
  "singleQuote": true,
  "trailingComma": "none",
  "jsxSingleQuote": false,
  "bracketSpacing": true,
  "importOrder": ["^@configs/(.*)$", "^@libs/(.*)$", "^[./]", "<THIRD_PARTY_MODULES>"],
  "importOrderSortSpecifiers": true
}
